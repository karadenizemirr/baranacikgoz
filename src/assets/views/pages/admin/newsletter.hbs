<section class="page-title">
    <div class="container">
        <div class="inner align-center">
            <h1 class="title">Tüm Abonelikler</h1>
            <div class="desc">Abone olan tüm kullanıcılara buradan ulaşabilirsiniz..</div>
        </div>
    </div>
</section><!-- PAGE TITLE -->
<div class="site-content">
    <div class="container">
        <div class="row">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">ID</th>
                        <th scope="col">Mail Adresi</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each data}}
                    <tr>
                        <th scope="row">{{this.id}}</th>
                        <td>{{this.email}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>